{
  "version": "0.1.0",
  "id": "stuff",
  "name": "Library Overview",
  "cells": {
    "pure-core": {
      "patterns": [
        "src/pure-core/**/*",
        "src/pure-core/abstractions/**/*",
        "src/pure-core/config/**/*",
        "src/pure-core/stores/**/*",
        "src/pure-core/types/**/*",
        "src/pure-core/utils/**/*"
      ],
      "coordinates": [0, 0],
      "priority": 10,
      "color": "#4CAF50"
    },
    "mcp-layer": {
      "patterns": [
        "src/mcp/**/*",
        "src/mcp/server/**/*",
        "src/mcp/tools/**/*",
        "src/mcp/types/**/*",
        "src/mcp/utils/**/*"
      ],
      "coordinates": [0, 1],
      "priority": 10,
      "color": "#2196F3"
    },
    "memory-palace": {
      "patterns": [
        "src/MemoryPalace.ts"
      ],
      "coordinates": [1, 0],
      "priority": 15,
      "color": "#FF9800"
    },
    "integration": {
      "patterns": [
        "src/index.ts",
        "src/lib.ts",
        "src/cli.ts",
        "src/node-adapters/**/*",
        "src/types.d.ts",
        "src/branding.ts"
      ],
      "coordinates": [1, 1],
      "priority": 5,
      "color": "#9C27B0"
    }
  },
  "overviewPath": "docs/architecture-overview.md"
}
